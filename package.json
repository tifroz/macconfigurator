{
  "name": "macconfigurator",
  "version": "1.0.0",
  "description": "Configuration manager",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist", "README.md"],
  "scripts": {
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc -b tsconfig.json",
    "build:client": "webpack --config src/build/webpack.config.js",
    "build:dev": "tsc -b tsconfig.json --watch",
    "dev": "npm run build:server && npm run dev:server",
    "dev:server": "node --watch ./dist/index.js",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build:server",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^2.1.1",
    "compression": "^1.7.5",
    "effect": "^3.16.13",
    "express": "^4.21.2",
    "express-basic-auth": "^1.2.1",
    "mongodb": "^6.12.0",
    "semver": "^7.6.3",
    "typescript": "^5.7.2",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.0",
    "@types/node": "^20.10.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-router-dom": "^5.3.3",
    "@types/semver": "^7.5.8",
    "autoprefixer": "^10.4.20",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.3",
    "postcss": "^8.4.49",
    "postcss-loader": "^8.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.17",
    "ts-loader": "^9.5.1",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  }
}
